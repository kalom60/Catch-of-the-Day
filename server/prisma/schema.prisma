// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

model Dish {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  name   String @unique
  price  Int
  status String
  desc   String
  image  String
}
